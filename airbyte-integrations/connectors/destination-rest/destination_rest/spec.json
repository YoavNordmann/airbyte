{
  "documentationUrl": "https://docs.airbyte.com/integrations/destinations/rest",
  "supported_destination_sync_modes": ["overwrite", "append"],

  "supportsIncremental": true,
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Destination Rest",
    "type": "object",
    "required": ["host", "port"],
    "additionalProperties": true,
    "properties": {
      "host": {
        "title": "Host",
        "description": "Hostname of the REST server.",
        "type": "string",
        "order": 0
      },
      "port": {
        "title": "Port",
        "description": "Port of the REST API.",
        "type": "int",
        "order": 1
      },
      "path": {
        "title": "Path",
        "description": "Path of the REST API.",
        "type": "string",
        "order": 1
      },
      "key_name": {
        "title": "key name",
        "description": "The key name in the JSON object under which all data will be sent",
        "type": "string",
        "order": 2
      },
      "static_config": {
        "title": "Static configuration",
        "description": "Static Configuration to be sent to the rest endpoint.",
        "type": "string",
        "order": 3,
        "multiline": true
      }

    }
  }
}
